#!/bin/bash
realpath=`readlink -fn $0`
export OPENSSL_CONF=/home/mk-build1/haotian/LE11_LATEST_AP/oe-core/build/tmp-glibc/sysroots/x86_64-linux/usr/lib/ssl/openssl.cnf SSL_CERT_DIR=/home/mk-build1/haotian/LE11_LATEST_AP/oe-core/build/tmp-glibc/sysroots/x86_64-linux/usr/lib/ssl/certs SSL_CERT_FILE=/home/mk-build1/haotian/LE11_LATEST_AP/oe-core/build/tmp-glibc/sysroots/x86_64-linux/usr/lib/ssl/cert.pem OPENSSL_ENGINES=/home/mk-build1/haotian/LE11_LATEST_AP/oe-core/build/tmp-glibc/sysroots/x86_64-linux/usr/lib/ssl/engines
exec -a `dirname $realpath`/openssl `dirname $realpath`/openssl.real "$@"
